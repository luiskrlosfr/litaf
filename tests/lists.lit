litaf start : 
    
    lis list[5], list2[5], test[5]
    
    fun SEARCH() is void 
        boo found
        int x, index, num
        str word
        found = false
        x = 0
        index = 0
        out("Ingresa la palabra a buscar")
        in(word)
        loop from x upto 4
            if(list2[x] == word)
                found = true
                index = x
            end
        by +1 end
        if (found == true)
            out("Word '", word, "' found at index ", index, "!", "n/")
        else
            out("Word '", word, "' not found", "n/")
        end
    end

    fun VECTOR() is void
        int x, i
        flo y
        str z
        cha a
        boo b 
        in(x)
        in(y)
        in(z)
        in(a)
        in(b)
        list2[0] = x
        list2[1] = y
        list2[2] = z
        list2[3] = a
        list2[4] = b
        i = 0
        out("[")
        loop from i upto 4
            out(list2[i], " ")
        by +1 end
        out("]", "n/")
    end

    fun BRUTE_FORCE_SORT() is void
        flo temp
        int i, j
        i = 0
        j = 0
        temp = 0
        loop from i upto 4
            j = i + 1
            loop from j upto 4
                if(test[j] < test[i])
                    temp = test[i]
                    test[i] = test[j]
                    test[j] = temp
                end
            by +1 end
        by +1 end
        out("[")
        i = 0
        loop from i upto 4
            out(test[i], " ")
        by +1 end
        out("]", "n/")
    end

    main is int 
        int x, num
        test[0] = 18.3
        test[1] = 20
        test[2] = 0.2
        test[3] = 55
        test[4] = 55.2
        % loop from x upto 4
            out("Ingresa el valor de N en la casilla ", x, "n/")
            in(num)
            list[x] = num
        by +1 end %
        BRUTE_FORCE_SORT()
        % VECTOR()
        SEARCH() %
    with 0 end 
end
